<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:datepicker='urn:import:com.google.gwt.user.datepicker.client'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'>

  <g:HTMLPanel>



   <table>
   <tr>
   <td>
     			<label>Start Date</label><br/>
                     <datepicker:DateBox ui:field="startDatePicker"/>   
   </td>
   <td>
        			<label>End Date		</label><br/>
                     <datepicker:DateBox ui:field="endDatePicker"/>
   </td>
   
   <td>
   	<label>Location</label><br/>
       <g:ListBox ui:field="locationListBox"/>
       
       <br/>
       <label><g:SimpleCheckBox ui:field="showDeleted"/><span>Show Deleted</span></label>
   </td>
   </tr>

</table>
     			

<g:Button ui:field="searchButton">Search</g:Button>
    
<g:Button ui:field="createButton">Create Closing</g:Button>


<g:SimplePanel ui:field="resultsPanel"></g:SimplePanel>
    
      
          		  		
      		
</g:HTMLPanel>

</ui:UiBinder>